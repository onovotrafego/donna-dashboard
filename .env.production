# Autenticação
SESSION_SECRET=gerar_uma_chave_segura_32_caracteres
JWT_SECRET=gerar_outra_chave_segura_32_caracteres

# Banco de Dados
DATABASE_URL=postgresql://usuario:senha@localhost:5432/nome_do_banco
